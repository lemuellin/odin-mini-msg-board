extends layout 

block content 
    h1 New Message 
    <form method="POST" action="/new">
        <label for="username">User Name</label>
        <input type="text" id="username" name="username"/>

        <label for="msg">Message</label>
        <input type="text" id="msg" name="msg"/>
    
        <button id="submit">Submit</button>
    </form>